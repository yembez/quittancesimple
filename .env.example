# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Stripe Configuration
# Get your publishable key from: https://dashboard.stripe.com/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Powens Configuration (for bank aggregation)
# Get these from your Powens dashboard
POWENS_CLIENT_ID=your-powens-client-id
POWENS_CLIENT_SECRET=your-powens-client-secret
POWENS_API_URL=https://api-sandbox.powens.com  # sandbox or production
POWENS_REDIRECT_URI=https://app.quittancesimple.fr/bank-sync

# Stripe Price IDs - Mensuel
# Quittance Automatique - Tarification par paliers
VITE_STRIPE_PRICE_AUTO_TIER1=price_xxx  # 1-2 logements - 0,99€/mois
VITE_STRIPE_PRICE_AUTO_TIER2=price_xxx  # 3-5 logements - 1,49€/mois
VITE_STRIPE_PRICE_AUTO_TIER3=price_xxx  # 5+ logements - 2,49€/mois

# Stripe Price IDs - Annuel
VITE_STRIPE_PRICE_AUTO_TIER1_YEARLY=price_xxx  # 1-2 logements - 9,90€/an
VITE_STRIPE_PRICE_AUTO_TIER2_YEARLY=price_xxx  # 3-5 logements - 14,90€/an
VITE_STRIPE_PRICE_AUTO_TIER3_YEARLY=price_xxx  # 5+ logements - 24,90€/an

# Quittance Connectée+ - Ancien système
VITE_STRIPE_PRICE_PLUS_FIRST=price_xxx
VITE_STRIPE_PRICE_PLUS_ADDITIONAL=price_xxx
